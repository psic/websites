		<script type="text/javascript">
		$(document).ready( function()
		{ 
		
		 		
               $('#contenu').load("html/accueil.html");
		 
		
		 $("#menu li").click(function()
		   {
		  	 $("#menu li").removeClass("active");		
			 jQuery(this).addClass("active");	 
			 switch(this.id){  
        		case "accueil": 
		            		$('#contenu').load("html/accueil.html");
           			 	break;
			case "comp":
					$('#contenu').load("html/competences.html");
					break;
			case "blog":
				$('#contenu').load("html/blog/");
					break;
			case "quijesuis":
					$('#contenu').load("html/quijesuis.html");
					break;
			case "liens":
				$('#contenu').load("html/liens.html");
					break;
				
				}
		   });	
		} ); 
	</script> 
